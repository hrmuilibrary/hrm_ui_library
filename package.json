{
  "name": "hrm_ui_lib",
  "version": "2.4.10",
  "description": "UI library for Dino",
  "private": false,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "version": "echo $npm_package_version",
    "name": "echo $npm_package_name",
    "start": "react-scripts start",
    "build": "rollup -c",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006 public",
    "build-storybook": "storybook build",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"./{src,stories}/**/**.{js,jsx,ts,tsx}\"",
    "check": "npm run prettier && npm run lint && tsc",
    "install-latest": "pnpm install",
    "import-all-svg-components": "node ./bin/import_all_svg_components.js",
    "publish-alpha-version": "cd \"./dist/\" && npm publish --tag alpha",
    "generate-svg-component": "node ./bin/generate_svg_component.js && npm run import-all-svg-components && npm run check",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hrmuilibrary/hrm_ui_library.git"
  },
  "homepage": "https://github.com/hrmuilibrary/hrm_ui_library",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "yup": ">=1.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/is-prop-valid": "^1.3.1",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^5.1.1",
    "@ncdai/react-wheel-picker": "^1.0.14",
    "@tanstack/react-table": "^8.21.3",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "framer-motion": "12.23.12",
    "react-datepicker": "^8.4.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.59.0",
    "react-input-mask": "^2.0.4",
    "react-loading-skeleton": "^3.5.0",
    "react-number-format": "^5.4.4",
    "react-paginate": "^8.3.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-table": "^7.8.0",
    "react-toastify": "11.0.5",
    "react-window": "^1.8.11",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@babel/preset-env": "7.28.0",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^24.0.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "rollup": "^4.44.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.8.3",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
