{
  "homepage": "https://github.com/hrmuilibrary/hrm_ui_library",
  "name": "hrm_ui_lib",
  "version": "3.1.16",
  "description": "UI library for Dino",
  "private": false,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "version": "echo $npm_package_version",
    "name": "echo $npm_package_name",
    "build": "vite build",
    "start": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"./{src,stories}/**/**.{js,jsx,ts,tsx}\"",
    "check": "npm run prettier && npm run lint && tsc",
    "install-latest": "pnpm install",
    "publish-alpha-version": "cd \"./dist/\" && npm publish --tag alpha",
    "generate-svg-component": "node ./bin/generate_svg_component.js && npm run check",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "yup": ">=1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@storybook/react": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@types/node": "^24.0.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.7",
    "@types/react-input-mask": "^3.0.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-table": "^7.7.20",
    "@types/react-window": "^1.8.8",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.3.4",
    "ajv": "8.17.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.0.18",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass-loader": "^16.0.6",
    "semantic-release": "^24.2.6",
    "storybook": "^10.1.11",
    "storybook-design-token": "^5.0.0",
    "terser": "^5.44.1",
    "typescript": "^5.8.3",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/is-prop-valid": "^1.3.1",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^5.1.1",
    "@ncdai/react-wheel-picker": "^1.0.14",
    "@tanstack/react-table": "^8.21.3",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "motion": "^12.23.12",
    "react-datepicker": "^8.4.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.59.0",
    "react-input-mask": "^2.0.4",
    "react-loading-skeleton": "^3.5.0",
    "react-number-format": "^5.4.4",
    "react-paginate": "^8.3.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-table": "^7.8.0",
    "react-toastify": "11.0.5",
    "react-window": "^1.8.11",
    "sass": "^1.89.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hrmuilibrary/hrm_ui_library.git"
  }
}
